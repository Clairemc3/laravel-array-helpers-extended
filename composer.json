{
    "name": "clairedv/laravel-array-helpers",
    "description": "Some static helpers for querying and transforming php arrays",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ClaireDv\\LaravelArrayHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClaireDv\\LaravelArrayHelpers\\Tests\\": "tests/"
        }
      },
    "authors": [
        {
            "name": "Claire Mcevoy",
            "email": "clairemc3@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^7|^8|^9|^10"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true,
      "preferred-install": "dist",
      "optimize-autoloader": true
    }

}
